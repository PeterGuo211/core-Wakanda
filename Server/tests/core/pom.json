{
    "description": "Main GoNoGo Server tests",
    "kind": "shell",
    "oldJenkinsName": "GoNoGoServer",
    "tester": "geoffrey.beauny@4d.com",
    "visibility": "public",
    "developer": "sergiy.temnikov@4d.com",
    "gonogo": true,
    "builders": {
    	"Mac": {
    		"build": [
    			{
    				"type": "Shell",
    				"command": "$UTILS_TESTS_PATH/Mac/GoNoGoServer/run.sh $SERVER_TESTS_PATH $UTILS_TESTS_PATH $WORKSPACE $BUILD_TEST_DIR"
			}
    		]
    	},
    	"Windows": {
    		"build": [
    			{
    				"type": "BatchFile",
    				"command": "%UTILS_TESTS_PATH%\\Win\\GoNoGoServer\\run.bat %SERVER_TESTS_PATH% %UTILS_TESTS_PATH% &quot;%WORKSPACE%&quot; %BUILD_TEST_DIR%"
			}
    		]
    	},
    	"Linux": {
    		"build": [
    			{
    				"type": "Shell",
    				"command": "$UTILS_TESTS_PATH/linux/GoNoGoServer/run.sh $SERVER_TESTS_PATH $UTILS_TESTS_PATH $WORKSPACE $BUILD_TEST_DIR"
			}
    		]
    	}
    }
}